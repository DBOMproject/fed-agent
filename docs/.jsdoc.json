{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["app.js", "package.json"],
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": true,
      "useLongnameInNav": false,
      "showInheritedInNav": true,
      "systemName": "DBoM Federation Agent for Node.js",
      "theme": "cosmo"
    },
    "opts": {
      "destination": "docs/gen",
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "template": "./node_modules/minami"
    }
  }